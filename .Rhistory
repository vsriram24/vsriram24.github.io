# MolRegEx ----
MolRegEx <- read.xlsx(
getLatestFile(path = "/Users/vsriram/Desktop/", pattern = "MolRegEx_")[1, "fullname"]) |>
group_by(Pro.Name, Pro.Type) |>
mutate(RegEx.list = paste(Pro.RegEx, collapse = ")|(")) |>
select(-Pro.RegEx) |>
unique() |>
pivot_wider(
id_cols = Pro.Name,
names_from = Pro.Type, names_glue = "{Pro.Type}.{.value}",
values_from = RegEx.list
) |>
mutate(Name.RegEx = paste0("(?i)(", Name.RegEx.list, ")")) |>
mutate(Abbreviation.RegEx = paste0("(?i)(", Abbreviation.RegEx.list, ")"))
